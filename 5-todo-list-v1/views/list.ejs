<%-include('header') %>

<div class="box" id="heading">
    <h1> <%=Title%>.</h1>
</div>

<div class="box">

    <% tasksArr.forEach((item)=>{%>
    <div class="item">
        <input type="checkbox">
        <p><%=item%></p>

    </div>
    <%});%>

    <form class="item" method="post">
        <input type="text" name="task" placeholder="New Task" autocomplete="off">
        <button type="submit" name="listname" value=<%=Title%>>+</button>
    </form>
</div>



<%- include('footer')%>